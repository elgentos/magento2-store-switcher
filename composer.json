{
    "name": "elgentos/magento2-store-switcher",
    "description": "Magento2 Store Switcher",
    "type": "magento2-module",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "elgentos ecommerce solutions",
            "email": "info@elgentos.nl"
        }
    ],
    "require": {
        "php": "^8.3",
        "magento/framework": "*",
        "hyva-themes/magento2-default-theme": "^1.3"
    },
    "autoload": {
        "psr-4": {
